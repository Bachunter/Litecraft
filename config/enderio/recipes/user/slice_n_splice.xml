<?xml version="1.0" encoding="UTF-8"?>
<enderio:recipes xmlns:enderio="http://enderio.com/recipes" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://enderio.com/recipes recipes.xsd ">
  <recipe name="Tormented Enderman Head">
    <slicing energy="20000">
      <input name="plateSoularium"/><input name="enderio:block_enderman_skull:0"/><input name="plateSoularium"/>
      <input name="minecraft:potion" nbt='{Potion:"minecraft:water"}'/><input name="CAPACITOR1"/><input name="minecraft:potion" nbt='{Potion:"minecraft:water"}'/>
      <output name="enderio:block_enderman_skull:2"/>
    </slicing>
  </recipe>

  <recipe name="Electrode Skull">
    <slicing energy="20000">
      <input name="plateEnergeticAlloy"/><input name="minecraft:skull:2"/><input name="plateEnergeticAlloy"/>
      <input name="plateSilicon"/><input name="CAPACITOR1"/><input name="plateSilicon"/>
      <output name="skullZombieElectrode"/>
    </slicing>
  </recipe>

   <recipe name="Controller Skull">
    <slicing energy="20000">
      <input name="plateSoularium"/><input name="minecraft:skull:2"/><input name="plateSoularium"/>
      <input name="plateSilicon"/><input name="dustRedstone"/><input name="plateSilicon"/>
      <output name="skullZombieController"/>
    </slicing>
  </recipe>

   <recipe name="Ender Resonator">
    <slicing energy="20000">
      <input name="plateSoularium"/><input name="enderio:block_enderman_skull:0"/><input name="plateSoularium"/>
      <input name="plateSilicon"/><input name="plateVibrantAlloy"/><input name="plateSilicon"/>
      <output name="skullEnderResonator"/>
    </slicing>
  </recipe>

   <recipe name="Skeletal Contractor">
    <slicing energy="20000">
      <input name="plateSoularium"/><input name="minecraft:skull:0"/><input name="plateSoularium"/>
      <input name="minecraft:rotten_flesh"/><input name="plateSilicon"/><input name="minecraft:rotten_flesh"/>
      <output name="skullSkeletalContractor"/>
    </slicing>
  </recipe>

  <recipe name="Guardian Diode">
    <slicing energy="20000">
      <input name="plateEnergeticAlloy"/><input name="gemPrismarine"/><input name="plateEnergeticAlloy"/>
      <input name="dustPrismarine"/><input name="plateSilicon"/><input name="dustPrismarine"/>
      <output name="skullGuardianDiode"/>
    </slicing>
  </recipe>
</enderio:recipes>
